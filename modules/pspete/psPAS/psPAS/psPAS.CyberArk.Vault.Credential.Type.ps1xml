<?xml version="1.0" encoding="utf-8" ?>
<Types>
	<Type>
		<Name>psPAS.CyberArk.Vault.Credential</Name>
		<Members>
			<ScriptMethod>
				<Name>ToSecureString</Name>
				<Script>
					$this | Select-Object -ExpandProperty Password | ConvertTo-SecureString -AsPlainText -Force
				</Script>
			</ScriptMethod>
		</Members>
	</Type>
</Types>